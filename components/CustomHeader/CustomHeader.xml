<?xml version="1.0" encoding="utf-8"?>
<component name="CustomHeader" extends="Group">
  <script type="text/brightscript" uri="CustomHeader.brs" />
  <children>
    <Group>
      <Poster
          id="logo"
          uri="pkg:/images/logo.png"
          width="60"
          height="60"
          translation="[40,15]"
      />

      <Label
          id="clock"
          font="font:MediumSystemFont"
          color="#F9F9F9"
          width="100"
          height="30"
          translation="[1100,35]"
          horizAlign="right"
      />
      
      <Rectangle
          id="bottomBorder"
          height="2"
          width="1280"
          color="#0f3657"
          translation="[0,90]"
      />
    </Group>
  </children>
</component>